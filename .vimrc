filetype plugin indent on
colo desert
syntax on
set tabstop=4
set shiftwidth=4
set expandtab
set spell spelllang=en_us
hi clear SpellBad
hi SpellBad cterm=underline

nnoremap <C-J> <C-W><C-J>
nnoremap <C-K> <C-W><C-K>
nnoremap <C-L> <C-W><C-L>
nnoremap <C-H> <C-W><C-H>

set splitbelow
set splitright
